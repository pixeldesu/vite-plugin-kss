{
  "name": "vite-plugin-kss",
  "version": "1.0.1",
  "description": "Plugin to integrate KSS into the Vite bundling process",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup-node src/index.ts --dts",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint .",
    "example": "vite build --config example/vite.config.ts",
    "example-dev": "vite serve --config example/vite.config.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixeldesu/vite-plugin-kss.git"
  },
  "keywords": [
    "vite-plugin",
    "kss",
    "kss-node"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pixeldesu/vite-plugin-kss/issues"
  },
  "homepage": "https://github.com/pixeldesu/vite-plugin-kss#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "9.0.0",
    "prettier": "^3.0.3",
    "rollup": "^3.24.1",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  },
  "dependencies": {
    "@types/kss": "^3.0.2",
    "@types/node": "^20.2.6",
    "kss": "^3.1.0"
  }
}
